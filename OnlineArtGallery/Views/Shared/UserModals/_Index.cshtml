
<div class="modal fade" id="signin-modal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-body">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true"><i class="icon-close"></i></span>
                </button>

                <div class="form-box">
                    <div class="form-tab">
                        <ul class="nav nav-pills nav-fill" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active" id="signin-tab" data-toggle="tab" href="#signin" role="tab" aria-controls="signin" aria-selected="true">Sign In</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="register-tab" data-toggle="tab" href="#register" role="tab" aria-controls="register" aria-selected="false">Register</a>
                            </li>
                        </ul>
                        <div class="tab-content" id="tab-content-5">
                            <div class="tab-pane fade show active" id="signin" role="tabpanel" aria-labelledby="signin-tab">
                                <form id="signInForm" action="/FEAuth/SignIn" method="post">

                                    <div class="form-group">
                                        <label for="singin-email">Email address *</label>
                                        <input type="text" class="form-control" id="singin-email" name="user_email" required>
                                    </div><!-- End .form-group -->

                                    <div class="form-group">
                                        <label for="singin-password">Password *</label>
                                        <input type="password" class="form-control" id="singin-password" name="user_password" required>
                                    </div><!-- End .form-group -->

                                    <div class="form-footer">
                                        <button type="submit" id="btnLogin" class="btn btn-outline-primary-2">
                                            <span>LOG IN</span>
                                            <span class="" id="spinnerLogin" role="status" aria-hidden="true"></span>
                                        </button>

                                        <div class="custom-control custom-checkbox">
                                            <input type="checkbox" class="custom-control-input" id="signin-remember">
                                            <label class="custom-control-label" for="signin-remember">Remember Me</label>
                                        </div><!-- End .custom-checkbox -->

                                        <a href="#" class="forgot-link">Forgot Your Password?</a>
                                    </div><!-- End .form-footer -->
                                </form>
                                <div class="form-choice">
                                    <p class="text-center">or sign in with</p>
                                    <div class="row">
                                        <div class="col-sm-6">
                                            <div
                                               id="g_id_onload"
                                               data-client_id="288616480418-267rh0ku0btqdksfr498k36pnsoqo2t6.apps.googleusercontent.com"
                                               data-callback="googleLogin">
                                            </div>
                                            <div class="g_id_signin" data-type="standard">

                                            </div>

                                        </div><!-- End .col-6 -->
                                        <div class="col-sm-6">
                                            <a class="btn btn-login btn-f" onclick="fbLogin();">
                                                <i class="icon-facebook-f"></i>
                                                Login With Facebook
                                            </a>
                                        </div><!-- End .col-6 -->
                                    </div><!-- End .row -->
                                </div><!-- End .form-choice -->
                            </div><!-- .End .tab-pane -->
                            <div class="tab-pane fade" id="register" role="tabpanel" aria-labelledby="register-tab">
                                <form id="SignUpForm">
                                    <div class="d-flex">
                                        <div class="form-group mr-3">
                                            <label for="singin-fname">First name *</label>
                                            <input type="text" class="form-control" id="singin-fname" name="user_fname" required>
                                        </div><!-- End .form-group -->
                                        <div class="form-group ml-5">
                                            <label for="singin-lname">Last name *</label>
                                            <input type="text" class="form-control" id="singin-lname" name="user_lname" required>
                                        </div><!-- End .form-group -->
                                    </div>
                                    <div class="form-group">
                                        <label for="register-email">Your email address *</label>
                                        <input type="email" class="form-control" id="register-email" name="user_email" required>
                                    </div><!-- End .form-group -->

                                    <div class="form-group">
                                        <label for="register-password">Password *</label>
                                        <input type="password" class="form-control" id="register-password" name="user_password" required>
                                    </div><!-- End .form-group -->

                                    <div class="form-group">
                                        <label for="confirm-password">Confirm Password *</label>
                                        <input type="password" class="form-control" id="confirm-password" required>
                                    </div><!-- End .form-group -->

                                    <div class="form-footer">
                                        <button type="submit" id="btnRegis" class="btn btn-outline-primary-2">
                                            <span>SIGN UP</span>
                                            <span class="" id="spinnerRegis" role="status" aria-hidden="true"></span>
                                        </button>

                                        <div class="custom-control custom-checkbox">
                                            <input type="checkbox" class="custom-control-input" id="register-policy" required>
                                            <label class="custom-control-label" for="register-policy">I agree to the <a href="#">privacy policy</a> *</label>
                                        </div><!-- End .custom-checkbox -->
                                    </div><!-- End .form-footer -->
                                </form>
                                <div class="form-choice">
                                    <p class="text-center">or sign in with</p>
                                    <div class="row">
                                        <div class="col-sm-6">
                                            <div id="g_id_onload"
                                                 data-client_id="288616480418-267rh0ku0btqdksfr498k36pnsoqo2t6.apps.googleusercontent.com"
                                                 data-callback="googleLogin">
                                            </div>
                                            <div class="g_id_signin" data-type="standard">

                                            </div>
                                        </div><!-- End .col-6 -->
                                        <div class="col-sm-6">
                                            <a href="#" class="btn btn-login btn-f" onclick="fbLogin();">
                                                <i class="icon-facebook-f"></i>
                                                Login With Facebook
                                            </a>
                                        </div><!-- End .col-6 -->
                                    </div><!-- End .row -->
                                </div><!-- End .form-choice -->
                            </div><!-- .End .tab-pane -->
                        </div><!-- End .tab-content -->
                    </div><!-- End .form-tab -->
                </div><!-- End .form-box -->
            </div><!-- End .modal-body -->
        </div><!-- End .modal-content -->
    </div><!-- End .modal-dialog -->
</div>

<!-- TOAST -->
<div class="position-fixed p-3" style="z-index: 9999; bottom: 0; right: 0;">
    <div class="toast align-items-center text-white bg-primary border-0" id="toast" role="alert" aria-live="assertive" aria-atomic="true" style="min-height:30px; width: 30vw">
        <div class="d-flex">
            <div class="toast-body pl-3" style="font-size: 15px">

            </div>
        </div>
    </div>
</div>

